{
    "extends": ["@rocketseat/eslint-config/react", "plugin:prettier/recommended"],
    "plugins": ["simple-import-sort", "prettier"],
    "rules": {
      "simple-import-sort/imports": "error",
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "endOfLine": "auto"
        }
      ]
    }
  }